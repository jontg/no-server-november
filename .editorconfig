# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# indentation of 2 spaces
indent_style = space
indent_size = 2

# dont leave random whitespace
trim_trailing_whitespace = true

# utf8 charset
charset = utf8

max_line_length = 100
